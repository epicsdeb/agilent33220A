example_registerRecordDeviceDriver(pdbbase)

drvAsynIPPortConfigure("FUNC", "172.23.243.212:5025", 100, 0, 0)

# Configure StreamDevice paths
epicsEnvSet "STREAM_PROTOCOL_PATH", "$(AGILENT33220A)/data"
